# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:5000/api
NEXT_PUBLIC_SOCKET_URL=http://localhost:5000

# Application Configuration
NEXT_PUBLIC_APP_NAME=MediQuick
NEXT_PUBLIC_APP_VERSION=1.0.0

# Payment Gateway (Public Keys only)
NEXT_PUBLIC_RAZORPAY_KEY_ID=your-razorpay-public-key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Google Maps API (if using location services)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Analytics (if using)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your-google-analytics-id

# Feature Flags
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true